{
  "name": "marshal-snippets",
  "displayName": "marshal Snippets",
  "version": "0.1.7",
  "publisher": "marshal",
  "author": "marshal93@163.com",
  "categories": [
    "Snippets"
  ],
  "description": "marshal Snippets for VS Code",
  "repository": {
    "type": "git",
    "url": "https://github.com/marshal-base/marshal-snippets.git"
  },
  "icon": "images/logo.png",
  "keywords": [
    "marshal",
    "vue",
    "snippets"
  ],
  "engines": {
    "vscode": "^1.74.0"
  },
  "main": "./out/index.js",
  "devDependencies": {
    "@marshal93/eslint-config": "^1.0.5",
    "@types/node": "^16.18.34",
    "@types/pretty": "^2.0.3",
    "@types/vscode": "^1.73.0",
    "eslint": "^8.26.0",
    "typescript": "5.1.6"
  },
  "license": "MIT",
  "contributes": {
    "commands": [
      {
        "command": "extension.openElement",
        "title": "Open Element-UI on VSCode"
      },
      {
        "command": "extension.openUri",
        "title": "Open uri on VSCode"
      }
    ],
    "snippets": [
      {
        "language": "javascript",
        "path": "./out/snippets/ui.json"
      },
      {
        "language": "javascript",
        "path": "./out/snippets/common.json"
      },
      {
        "language": "html",
        "path": "./out/snippets/ui.json"
      },
      {
        "language": "vue",
        "path": "./out/snippets/ui.json"
      },
      {
        "language": "vue",
        "path": "./out/snippets/common.json"
      },
      {
        "language": "vue",
        "path": "./out/snippets/vue-el.json"
      },
      {
        "language": "vue",
        "path": "./out/snippets/vant.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "sh build.sh",
    "compile": "sh build.sh",
    "watch": "sh build.sh --watch",
    "lint": "eslint \"src/**/*.ts\" --fix"
  },
  "dependencies": {
    "shelljs": "^0.8.5"
  }
}
