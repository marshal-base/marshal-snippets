{
  "Vue: el-dialog-init": {
    "prefix": "VueInitElDialog",
    "body": [
      "<template>",
      "<el-dialog",
      "title=\"title\"",
      "width=\"800px\"",
      ":visible.sync=\"visible\"",
      "@close=\"handleCloseClick\">",
      "<el-form ref=\"refForm\" :model=\"form\" label-width=\"140px\" :rules=\"formRules\">",
      "<el-form-item label=\"数字类型输入框\" prop=\"demo1\">",
      "<el-input type=\"number\" placeholder=\"请输入金额\" v-model=\"form.demo1\" />",
      "</el-form-item>",
      "<el-form-item label=\"下拉选项\" prop=\"demo2\">",
      "<el-select v-model=\"form.demo2\" style=\"width: 100%;\">",
      "<el-option placeholder=\"请选择\" v-for=\"v in demo2List\" :key=\"v\" :label=\"v\" :value=\"v\" />",
      "</el-select>",
      "</el-form-item>",
      "<el-form-item label=\"开关\" prop=\"demo3\" v-if=\"form.demo2 === '服务商'\">",
      "<el-switch v-model=\"form.demo3\" />",
      "</el-form-item>",
      "<el-row type=\"flex\">",
      "<el-col :span=\"12\">",
      "<el-form-item label=\"选择时间\" prop=\"demo4\">",
      "<el-date-picker type=\"date\" placeholder=\"选择日期\" v-model=\"form.demo4\" style=\"width: 100%;\" />",
      "</el-form-item>",
      "</el-col>",
      "<el-col style=\"line-height: 40px;text-align: center;\" :span=\"1\">-</el-col>",
      "<el-col :span=\"11\">",
      "<el-form-item prop=\"demo5\" label-width=\"0px\">",
      "<el-time-picker placeholder=\"选择时间\" v-model=\"form.demo5\" style=\"width: 100%;\" />",
      "</el-form-item>",
      "</el-col>",
      "</el-row>",
      "<el-form-item label=\"多选\" prop=\"demo6\">",
      "<el-checkbox-group v-model=\"form.demo6\">",
      "<el-checkbox :label=\"v\" v-for=\"v in demo6List\" :key=\"v\" />",
      "</el-checkbox-group>",
      "</el-form-item>",
      "<el-form-item label=\"单选\" prop=\"demo7\">",
      "<el-radio-group v-model=\"form.demo7\">",
      "<el-radio :label=\"v\" v-for=\"v in demo7List\" :key=\"v\" />",
      "</el-radio-group>",
      "</el-form-item>",
      "<el-form-item label=\"textarea\" prop=\"demo8\">",
      "<el-input",
      "type=\"textarea\"",
      ":rows=\"3\"",
      "placeholder=\"请输入，最多 200 个字符\"",
      "maxLength=\"200\"",
      "v-model=\"form.demo8\">",
      "</el-input>",
      "</el-form-item>",
      "</el-form>",
      "<div slot=\"footer\" style=\"text-align: center;\">",
      "<el-button @click=\"handleCloseClick\">取 消</el-button>",
      "<el-button type=\"primary\" @click=\"handleSave\">确 定</el-button>",
      "</div>",
      "</el-dialog>",
      "</template>",
      "","<script>",
      "export default {",
      "name: 'test',",
      "data() {",
      "return {",
      "visible: false,",
      "loading: false,",
      "demo2List: ['服务商', '上汽'],",
      "demo6List: ['交强险', '商业险'],",
      "demo7List: ['单选 1', '单选 2'],",
      "providerList: [],",
      "form: {",
      "demo1: undefined,",
      "demo2: undefined,",
      "demo3: undefined,",
      "demo4: undefined,",
      "demo5: undefined,",
      "demo6: [],",
      "demo7: undefined,",
      "demo8: undefined",
      "},",
      "formRules: {",
      "demo1: [{ required: true, message: '不能为空', trigger: 'blur' }],",
      "demo2: [{ required: true, message: '不能为空', trigger: 'blur' }],",
      "demo3: [{ required: true, message: '不能为空', trigger: 'blur' }],",
      "demo4: [{ required: true, message: '不能为空', trigger: 'blur' }],",
      "demo5: [{ required: true, message: '不能为空', trigger: 'blur' }],",
      "demo6: [{ required: true, message: '不能为空', trigger: 'blur' }],",
      "demo7: [{ required: true, message: '不能为空', trigger: 'blur' }],",
      "demo8: [{ required: true, message: '不能为空', trigger: 'blur' }]",
      "}",
      "}",
      "},",
      "methods: {",
      "handleSave() {",
      "this.$refs.refForm.validate(valid => {",
      "if (valid) {",
      "this.handleSubmitClick()",
      "}",
      "})",
      "},",
      "handleCloseClick() {",
      "this.visible = false",
      "this.form = {",
      "demo1: undefined,",
      "demo2: undefined,",
      "demo3: undefined,",
      "demo4: undefined,",
      "demo5: undefined,",
      "demo6: [],",
      "demo7: undefined,",
      "demo8: undefined",
      "}",
      "},",
      "handleSubmitClick() {",
      "this.$confirm('请确认操作', '提示', {",
      "callback: action => {",
      "if (action === 'cancel') {",
      "return",
      "}",
      "",
      "const loading = this.$loading({",
      "lock: true,",
      "text: '加载中...',",
      "spinner: 'el-icon-loading',",
      "background: 'rgba(0, 0, 0, 0.7)'",
      "})",
      "this.loading = true",
      "",
      "setTimeout(() => {",
      "loading.close()",
      "this.$message.success('操作完成')",
      "this.loading = false",
      "this.$emit('onFinish')",
      "}, 2000)",
      "}",
      "})",
      "}",
      "}",
      "}",
      "</script>",
      ""
    ]
  }
}