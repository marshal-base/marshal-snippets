{
  "Vue: 创建弹层": {
    "prefix": "VueInitElDialog",
    "body": [
      "<template>",
      "\t<el-dialog title=\"title\" width=\"800px\" :visible.sync=\"visible\" @close=\"handleCloseClick\">",
      "\t\t<el-form ref=\"refForm\" :model=\"form\" label-width=\"140px\" :rules=\"formRules\">",
      "\t\t\t<el-form-item label=\"数字类型输入框\" prop=\"demo1\">",
      "\t\t\t\t<el-input type=\"number\" placeholder=\"请输入金额\" v-model=\"form.demo1\" />",
      "\t\t\t</el-form-item>",
      "\t\t\t<el-form-item label=\"下拉选项\" prop=\"demo2\">",
      "\t\t\t\t<el-select v-model=\"form.demo2\" style=\"width: 100%\">",
      "\t\t\t\t\t<el-option placeholder=\"请选择\" v-for=\"v in demo2List\" :key=\"v\" :label=\"v\" :value=\"v\" />",
      "\t\t\t\t</el-select>",
      "\t\t\t</el-form-item>",
      "\t\t\t<el-form-item label=\"开关\" prop=\"demo3\" v-if=\"form.demo2 === '服务商'\">",
      "\t\t\t\t<el-switch v-model=\"form.demo3\" />",
      "\t\t\t</el-form-item>",
      "\t\t\t<el-row type=\"flex\">",
      "\t\t\t\t<el-col :span=\"12\">",
      "\t\t\t\t\t<el-form-item label=\"选择时间\" prop=\"demo4\">",
      "\t\t\t\t\t\t<el-date-picker type=\"date\" placeholder=\"选择日期\" v-model=\"form.demo4\" style=\"width: 100%\" />",
      "\t\t\t\t\t</el-form-item>",
      "\t\t\t\t</el-col>",
      "\t\t\t\t<el-col style=\"line-height: 40px; text-align: center\" :span=\"1\">-</el-col>",
      "\t\t\t\t<el-col :span=\"11\">",
      "\t\t\t\t\t<el-form-item prop=\"demo5\" label-width=\"0px\">",
      "\t\t\t\t\t\t<el-time-picker placeholder=\"选择时间\" v-model=\"form.demo5\" style=\"width: 100%\" />",
      "\t\t\t\t\t</el-form-item>",
      "\t\t\t\t</el-col>",
      "\t\t\t</el-row>",
      "\t\t\t<el-form-item label=\"多选\" prop=\"demo6\">",
      "\t\t\t\t<el-checkbox-group v-model=\"form.demo6\">",
      "\t\t\t\t\t<el-checkbox :label=\"v\" v-for=\"v in demo6List\" :key=\"v\" />",
      "\t\t\t\t</el-checkbox-group>",
      "\t\t\t</el-form-item>",
      "\t\t\t<el-form-item label=\"单选\" prop=\"demo7\">",
      "\t\t\t\t<el-radio-group v-model=\"form.demo7\">",
      "\t\t\t\t\t<el-radio :label=\"v\" v-for=\"v in demo7List\" :key=\"v\" />",
      "\t\t\t\t</el-radio-group>",
      "\t\t\t</el-form-item>",
      "\t\t\t<el-form-item label=\"textarea\" prop=\"demo8\">",
      "\t\t\t\t<el-input type=\"textarea\" :rows=\"3\" placeholder=\"请输入，最多 200 个字符\" maxLength=\"200\" v-model=\"form.demo8\"></el-input>",
      "\t\t\t</el-form-item>",
      "\t\t</el-form>",
      "\t\t<div slot=\"footer\" style=\"text-align: center\">",
      "\t\t\t<el-button @click=\"handleCloseClick\">取 消</el-button>",
      "\t\t\t<el-button type=\"primary\" @click=\"handleSave\">确 定</el-button>",
      "\t\t</div>",
      "\t</el-dialog>",
      "</template>",
      "",
      "<script>",
      "export default {",
      "\tname: 'test',",
      "\tprops: {",
      "\t\tvisible: {",
      "\t\t\ttype: Boolean,",
      "\t\t\tdefault: false",
      "\t\t},",
      "\t\tcallback: {",
      "\t\t\ttype: Function,",
      "\t\t\tdefault: () => {}",
      "\t\t}",
      "\t},",
      "\tdata() {",
      "\t\treturn {",
      "\t\t\tloading: false,",
      "\t\t\tdemo2List: ['服务商', '上汽'],",
      "\t\t\tdemo6List: ['交强险', '商业险'],",
      "\t\t\tdemo7List: ['单选 1', '单选 2'],",
      "\t\t\tproviderList: [],",
      "\t\t\tform: {",
      "\t\t\t\tdemo1: undefined,",
      "\t\t\t\tdemo2: undefined,",
      "\t\t\t\tdemo3: undefined,",
      "\t\t\t\tdemo4: undefined,",
      "\t\t\t\tdemo5: undefined,",
      "\t\t\t\tdemo6: [],",
      "\t\t\t\tdemo7: undefined,",
      "\t\t\t\tdemo8: undefined",
      "\t\t\t},",
      "\t\t\tformRules: {",
      "\t\t\t\tdemo1: [{ required: true, message: '不能为空', trigger: 'blur' }],",
      "\t\t\t\tdemo2: [{ required: true, message: '不能为空', trigger: 'blur' }],",
      "\t\t\t\tdemo3: [{ required: true, message: '不能为空', trigger: 'blur' }],",
      "\t\t\t\tdemo4: [{ required: true, message: '不能为空', trigger: 'blur' }],",
      "\t\t\t\tdemo5: [{ required: true, message: '不能为空', trigger: 'blur' }],",
      "\t\t\t\tdemo6: [{ required: true, message: '不能为空', trigger: 'blur' }],",
      "\t\t\t\tdemo7: [{ required: true, message: '不能为空', trigger: 'blur' }],",
      "\t\t\t\tdemo8: [{ required: true, message: '不能为空', trigger: 'blur' }]",
      "\t\t\t}",
      "\t\t}",
      "\t},",
      "\tmethods: {",
      "\t\thandleSave() {",
      "\t\t\tthis.\\$refs.refForm.validate(valid => {",
      "\t\t\t\tif (valid) {",
      "\t\t\t\t\tthis.handleSubmitClick()",
      "\t\t\t\t}",
      "\t\t\t})",
      "\t\t},",
      "\t\thandleCloseClick() {",
      "\t\t\tthis.callback()",
      "\t\t\tthis.form = {",
      "\t\t\t\tdemo1: undefined,",
      "\t\t\t\tdemo2: undefined,",
      "\t\t\t\tdemo3: undefined,",
      "\t\t\t\tdemo4: undefined,",
      "\t\t\t\tdemo5: undefined,",
      "\t\t\t\tdemo6: [],",
      "\t\t\t\tdemo7: undefined,",
      "\t\t\t\tdemo8: undefined",
      "\t\t\t}",
      "\t\t},",
      "\t\thandleSubmitClick() {",
      "\t\t\tthis.\\$confirm('请确认操作', '提示', {",
      "\t\t\t\tcallback: action => {",
      "\t\t\t\t\tif (action === 'cancel') {",
      "\t\t\t\t\t\treturn",
      "\t\t\t\t\t}",
      "",
      "\t\t\t\t\tconst loading = this.\\$loading({",
      "\t\t\t\t\t\tlock: true,",
      "\t\t\t\t\t\ttext: '加载中...',",
      "\t\t\t\t\t\tspinner: 'el-icon-loading',",
      "\t\t\t\t\t\tbackground: 'rgba(0, 0, 0, 0.7)'",
      "\t\t\t\t\t})",
      "\t\t\t\t\tthis.loading = true",
      "",
      "\t\t\t\t\tsetTimeout(() => {",
      "\t\t\t\t\t\tloading.close()",
      "\t\t\t\t\t\tthis.\\$message.success('操作完成')",
      "\t\t\t\t\t\tthis.loading = false",
      "\t\t\t\t\t\tthis.callback('onFinish')",
      "\t\t\t\t\t}, 2000)",
      "\t\t\t\t}",
      "\t\t\t})",
      "\t\t}",
      "\t}",
      "}",
      "</script>",
      ""
    ]
  }
